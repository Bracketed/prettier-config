{
	"name": "@bracketed/prettier-configuration",
	"description": "Preset prettier configuration for Bracketed Softworks projects and community users' projects.",
	"packageManager": "yarn@4.9.2",
	"version": "1.0.0",
	"publishConfig": {
		"access": "public"
	},
	"license": "Apache-2.0",
	"author": "@bracketed",
	"main": "lib/cjs/index.cjs",
	"module": "lib/esm/index.mjs",
	"types": "lib/cjs/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./lib/esm/index.d.mts",
				"default": "./lib/esm/index.mjs"
			},
			"require": {
				"types": "./lib/cjs/index.d.ts",
				"default": "./lib/cjs/index.cjs"
			}
		}
	},
	"files": [
		"lib"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Bracketed/prettier-config"
	},
	"homepage": "https://github.com/Bracketed/prettier-config",
	"sideEffects": false,
	"bugs": {
		"url": "https://github.com/Bracketed/prettier-config/issues"
	},
	"keywords": [
		"bracketed",
		"typescript",
		"prettier",
		"formatter",
		"configuration",
		"preset"
	],
	"scripts": {
		"build": "tsup",
		"update": "yarn upgrade-interactive"
	},
	"devDependencies": {
		"@bracketed/tsup-configuration": "^1.0.5",
		"@bracketed/typescript-config": "^4.7.1",
		"@swc/core": "^1.13.1",
		"@types/node": "^24.0.3",
		"tslib": "^2.8.1",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@prettier/plugin-php": "^0.24.0",
		"@prettier/plugin-pug": "^3.4.0",
		"@prettier/plugin-ruby": "^4.0.4",
		"@prettier/plugin-xml": "^3.4.2",
		"prettier": "^3.6.2",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-embed": "^0.5.0",
		"prettier-plugin-jsdoc": "^1.3.3",
		"prettier-plugin-jsdoc-type": "^0.1.12",
		"prettier-plugin-merge": "^0.8.0",
		"prettier-plugin-prisma": "^5.0.0",
		"prettier-plugin-sh": "^0.18.0",
		"prettier-plugin-sort-imports": "^1.8.8",
		"prettier-plugin-sort-json": "^4.1.1",
		"prettier-plugin-sort-package-json": "^1.1.0",
		"prettier-plugin-sql": "^0.19.2",
		"prettier-plugin-tailwindcss": "^0.6.14"
	},
	"peerDependencies": {
		"prettier": "^3.6.2"
	}
}
